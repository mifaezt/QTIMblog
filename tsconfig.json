{
	"extends": "./.nuxt/tsconfig.json",
	"compilerOptions": {
		"strict": true,
		"types": ["nuxt"],
		"paths": {
			"~/*": ["./*"],
			"@/*": ["./*"]
		}
	},
	"include": [
		"./.nuxt",
		"./nuxt.config.ts",
		"./components/**/*",
		"./composables/**/*",
		"./layouts/**/*",
		"./pages/**/*",
		"./plugins/**/*",
		"./app.vue",
		"./error.vue"
	],
	"exclude": ["node_modules"]
}
